.indexContainer .header {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  height: 60rpx;
  padding: 10rpx;
}
.indexContainer .header .logo {
  width: 140rpx;
  height: 40rpx;
  margin: 10rpx 30rpx;
}
.indexContainer .header .search {
  height: 60rpx;
  width: 420rpx;
  background: #ededed;
  margin: 0 10rpx;
  border-radius: 10rpx;
}
.indexContainer .header .search input {
  height: 60rpx;
}
.indexContainer .header .search .placeholder {
  font-size: 24rpx;
}
.indexContainer .header button {
  width: 144rpx;
  height: 60rpx;
  font-size: 22rpx;
  color: #bb2c08;
  text-align: center;
  line-height: 60rpx;
  white-space: nowrap;
  overflow: hidden;
  -o-text-overflow: ellipsis;
     text-overflow: ellipsis;
}
.test {
  font-size: 0;
}


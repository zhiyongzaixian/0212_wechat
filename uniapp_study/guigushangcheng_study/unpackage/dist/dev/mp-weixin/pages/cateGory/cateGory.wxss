.cateGoryContainer .header {
  padding: 10rpx;
}
.cateGoryContainer .header .headerContent {
  width: 94%;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  background: #eee;
  margin: 0 auto;
  font-size: 26rpx;
  border-radius: 10rpx;
}
.cateGoryContainer .contentContainer {
  height: calc(100vh - 80rpx);
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  border-top: 1rpx solid #eee;
}
.cateGoryContainer .contentContainer .leftContainer {
  width: 20%;
  height: 100%;
  border-right: 1rpx solid #eee;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.cateGoryContainer .contentContainer .leftContainer .leftScroll {
  height: 100%;
}
.cateGoryContainer .contentContainer .leftContainer .leftScroll .navItem {
  position: relative;
  font-size: 24rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
}
.cateGoryContainer .contentContainer .leftContainer .leftScroll .navItem.activeClass:after {
  position: absolute;
  top: 10rpx;
  left: 10rpx;
  content: '';
  width: 2rpx;
  height: 60rpx;
  background: #bb2c08;
}
.cateGoryContainer .contentContainer .rightContainer {
  width: 80%;
  height: 100%;
}
.cateGoryContainer .contentContainer .rightContainer .rightScroll {
  height: 100%;
}
.cateGoryContainer .contentContainer .rightContainer .rightScroll .proInfo {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-flex-wrap: wrap;
      -ms-flex-wrap: wrap;
          flex-wrap: wrap;
}
.cateGoryContainer .contentContainer .rightContainer .rightScroll .proInfo .proImg {
  width: 520rpx;
  height: 190rpx;
  margin: 20rpx auto;
}
.cateGoryContainer .contentContainer .rightContainer .rightScroll .proInfo .proItem {
  width: 33.33%;
}
.cateGoryContainer .contentContainer .rightContainer .rightScroll .proInfo .proItem image {
  width: 90%;
  height: 140rpx;
}
.cateGoryContainer .contentContainer .rightContainer .rightScroll .proInfo .proItem view {
  font-size: 26rpx;
  text-align: center;
}
.test {
  font-size: 0;
}

